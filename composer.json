{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "astrotomic/laravel-translatable": "^11.9",
        "barryvdh/laravel-cors": "^3.0",
        "barryvdh/laravel-debugbar": "*",
        "darryldecode/cart": "^4.2",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^3.0",
        "gabrielbull/ups-api": "^2.1",
        "guzzlehttp/guzzle": "^7.5",
        "infyomlabs/adminlte-templates": "^3.0",
        "infyomlabs/laravel-generator": "^1.1",
        "intervention/image": "^2.7",
        "jenssegers/agent": "^2.6",
        "jenssegers/date": "^4.0",
        "laravel/framework": "^6.20.26",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.3",
        "mercadopago/dx-php": "^2.5",
        "prettus/l5-repository": "^2.8",
        "spatie/laravel-permission": "^5.3",
        "spatie/laravel-translatable": "^4.6",
        "yajra/laravel-auditable": "^4.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
